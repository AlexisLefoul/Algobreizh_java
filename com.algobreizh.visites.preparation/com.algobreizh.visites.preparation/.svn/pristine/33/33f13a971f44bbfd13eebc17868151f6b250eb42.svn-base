package com.algobreizh.visites.preparation.core.lib;

public class Rdv {
	private int IdRDV;
	private String idCommercial;
	private int Semaine;
	private String Nom;
	private String Prenom;
	private String Date;
	private String Lieu;
	
	public Rdv(int IdRDV, String idCommercial, int Semaine, String Nom, 
			String Prenom, String Date, String Lieu){
		this.IdRDV = IdRDV;
		this.idCommercial = idCommercial;
		this.Semaine = Semaine;
		this.Nom = Nom;
		this.Prenom = Prenom;
		this.Date = Date;
		this.Lieu = Lieu;
	}
	
	/*Getters*/
	public int getId(){ return IdRDV;}
	public String getidCommercial(){ return idCommercial;}
	public int getSemaine(){ return Semaine;}
	public String getNom(){ return Nom;}
	public String getPrenom(){ return Prenom;}
	public String getDate(){return Date;}
	public String getLieu(){return Lieu;}
	
	/*Setters*/
	public void setId(int IdRDV) { this.IdRDV = IdRDV; }
	public void setidCommercial(String idCommercial) { this.idCommercial = idCommercial; }
	public void setIdSemaine(int Semaine){ this.Semaine = Semaine;}
	public void setNom(String Nom) { this.Nom = Nom; }
	public void setPrenom(String Prenom) { this.Prenom = Prenom;}
	public void setDate(String Date) { this.Date = Date;}
	public void setLieu(String Lieu) { this.Lieu = Lieu;}
	
	
}
